{
  "name": "goose-chase",
  "author": "Toby Connor-Kebbell",
  "version": "0.0.0",
  "license": "MIT",
  "source": "src/main.ts",
  "targets": {
    "bundle": {
      "sourceMap": false,
      "optimize": true,
      "outputFormat": "commonjs",
      "distDir": "build"
    }
  },
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build --no-cache",
    "check": "tsc --noEmit",
    "lint": "eslint src --ext .ts --fix"
  },
  "pre-commit": [
    "build",
    "check",
    "lint"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.40.0",
    "parcel": "latest",
    "pre-commit": "^1.2.2",
    "typescript": "latest"
  }
}
